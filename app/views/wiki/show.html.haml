- provide :page_title,       t('.page_title',       title:       @wiki.title)
- provide :meta_keywords,    t('.meta_keywords',    keywords:    @wiki.seo_metadata.keywords)
- provide :meta_description, t('.meta_description', description: @wiki.seo_metadata.description)

%p
  %b Title:
  = @wiki.title
%p
  %b Text:
  = @wiki.text

= link_to 'Edit', edit_wiki_path(@wiki)
\|
= link_to 'Back', wiki_index_path
